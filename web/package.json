{
  "name": "agri-link",
  "version": "1.0.0",
  "description": "AI-powered agricultural crisis prevention system using Kestra AI Agents",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "kestra:deploy": "python scripts/deploy_flows.py",
    "kestra:demo": "python scripts/deploy_flows.py --demo --follow",
